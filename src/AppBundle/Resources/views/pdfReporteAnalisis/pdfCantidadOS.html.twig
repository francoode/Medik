<link rel="stylesheet" href="/var/www/html/ProyectoFinal/app/Resources/vendor/bootstrap/css/bootstrap.css">
<br>
<h3>Reporte cantidad de analisis por OS</h3>
Fecha Desde: {{ fi }} - Fecha Hasta: {{ ff }}

{% if cantidad is defined %}
    <div class="row">
        <div class="col-xs-7 col-md-offset-1">
            <b> Obra Social</b>

        </div>
        <div class="col-xs-2" style="text-align: center">
            <b>Cantidad</b>
        </div>
        <div class="col-xs-2" style="text-align: center">
            <b> Porcentaje %</b>
        </div>
    </div>

    {% for c in cantidad %}
        <div class="row">
            <div class="col-xs-7 col-md-offset-1">
                {{ c.name }}

            </div>
            <div class="col-xs-2" style="text-align: center">
                {{ c.cantidad }}
            </div>
            <div class="col-xs-2" style="text-align: center">
                {% set r = ((c.cantidad * 100) / total.total)%}
                {{ r | slice(0,4) }}%
            </div>
        </div>

    {% endfor %}
{% endif %}